.card

    .flex.gap-15px.flex-col.w-full
        h2(class='font-inter font-bold text-lg') Partners/Tier
        .divider

    div(class='grid grid-cols-2 lg:grid-cols-7-5 gap-15px')
        div(class='w-[140px]')
            canvas(class='doughnut-chart' data-labels='Platinum,Gold,Silver' data-values='24,32,44' data-colors='#0088C9,#39B4EF,#C4E9FB' data-label='Partners')

        div(class='flex flex-col gap-10px')
            div(class='flex flex-col w-max')
                span(class='text-textGray text-sm') Platinum Tier:
                .flex.items-center.gap-5px
                    div(class='w-[12px] h-[12px] block bg-blueChart2 rounded-full')
                    span.text-sm.font-semibold 24 Partners
            div(class='flex flex-col w-max')
                span(class='text-textGray text-sm') Gold Tier:
                .flex.items-center.gap-5px
                    div(class='w-[12px] h-[12px] block bg-blue rounded-full')
                    span.text-sm.font-semibold 32 Partners
            div(class='flex flex-col w-max')
                span(class='text-textGray text-sm') Silver Tier:
                .flex.items-center.gap-5px
                    div(class='w-[12px] h-[12px] block bg-blueChart rounded-full')
                    span.text-sm.font-semibold 44 Partners

