doctype html
html(lang="en")
    head
        - var title = 'Vendor Dashboard | Elioplus'
        include components/head.pug
    body.text-blackBlue.overflow-x-hidden.bg-gray2
        include components/header/menu-vendor.pug
        main
            // - var breadcrumbsParent = 'Partner Management'
            // - var breadcrumbsCurrent = 'Analytics'
            include components/header/header-vendor
            div(id="dashboardPage" class='py-50px lg:py-80px w-full')
                .container
                    div(id="dashboardPageContent" class='w-full flex flex-col gap-60px')

                        <!----------- Content Start ------------------------------>

                        - var title = 'Good Morning, Giannis'
                        include components/text-block.pug

                        div(class='grid grid-cols-2 lg:grid-cols-analytics gap-30px')
                            include components/cards/card-analytics-overview
                            include components/cards/card-analytics-deal-overview
                            include components/cards/card-analytics-leads-overview


                        div(class='grid grid-cols-2 lg:grid-cols-2 gap-30px')
                            div.flex.flex-col.gap-25px
                                include components/cards/card-analytics-messages
                                div(class='grid grid-cols-2 lg:grid-cols-5-7 gap-15px mt-auto')
                                    include components/cards/card-analytics-partners-overview
                                    include components/cards/card-analytics-tiers-overview
                            include components/cards/card-analytics-revenue-tier

                        div(class='grid grid-cols-2 lg:grid-cols-8-4 gap-30px')

                            include components/cards/card-analytics-active-partners
                            include components/cards/card-analytics-partners-country

                        div(class='w-full')
                            include components/cards/card-analytics-partners-revenue

                        div(class='grid grid-cols-2 gap-30px')
                            include components/cards/card-analytics-deals-closed
                            include components/cards/card-analytics-leads-closed


                        <!----------- Content End ------------------------------>

            include components/footer/footer.pug
        include components/scripts.pug
